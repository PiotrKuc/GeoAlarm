create sequence seq_kluby_lekkoatletyczne start with 1 increment by 1;create sequence seq_zawodnicy start with 1 increment by 1;create sequence seq_pracownicy start with 1 increment by 1;create sequence seq_masazysci start with 1 increment by 1;create sequence seq_psycholodzy start with 1 increment by 1;create sequence seq_trenerzy start with 1 increment by 1;create sequence seq_treningi start with 1 increment by 1;create sequence seq_dyscypliny start with 1 increment by 1;create sequence seq_turnieje start with 1 increment by 1;create sequence seq_obiekty start with 1 increment by 1;
create table kluby_lekkoatletyczne(  id_klubu number default seq_kluby_lekkoatletyczne.nextval not null,    constraint pk_id_klubu primary key (id_klubu));
create table zawodnicy(  id_zawodnika number default seq_zawodnicy.nextval not null ,  imie varchar2(15) not null,  nazwisko varchar2(25) not null,  pesel number(11) not null,  plec char(1) not null,  id_klubu number not null,    constraint pk_id_zawodnika primary key (id_zawodnika),  constraint fk_id_klubu foreign key (id_klubu) references kluby_lekkoatletyczne(id_klubu) on delete cascade,  constraint chck_plec check (plec in ('K','M')));
create table pracownicy(  id_pracownika number not null,  imie varchar2(15) not null,  nazwisko varchar2(25) not null,  pesel number(11) not null,  plec char(1) not null,  id_klubu number not null,  data_zatrudnienia date not null,    constraint pk_id_pracownika primary key (id_pracownika),  constraint fk_id_klubu foreign key (id_klubu) references kluby_lekkoatletyczne(id_klubu) on delete cascade);
create table trenerzy(
  id_trenera number default seq_trenerzy not null,  id_pracownika number not null,  
)
