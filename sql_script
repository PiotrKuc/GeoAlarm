create sequence seq_kluby_lekkoatletyczne start with 1 increment by 1;create sequence seq_zawodnicy start with 1 increment by 1;create sequence seq_pracownicy start with 1 increment by 1;create sequence seq_masazysci start with 1 increment by 1;create sequence seq_psycholodzy start with 1 increment by 1;create sequence seq_trenerzy start with 1 increment by 1;create sequence seq_treningi start with 1 increment by 1;create sequence seq_dyscypliny start with 1 increment by 1;create sequence seq_turnieje start with 1 increment by 1;create sequence seq_obiekty start with 1 increment by 1;
create table kluby_lekkoatletyczne(  id_klubu number default seq_kluby_lekkoatletyczne.nextval not null,    constraint pk_id_klubu primary key (id_klubu));
create table pracownicy(  id_pracownika number not null,  imie varchar2(15) not null,  nazwisko varchar2(25) not null,  pesel number(11) not null,  plec char(1) not null,  id_klubu number not null,  data_zatrudnienia date not null,    constraint pk_id_pracownika primary key (id_pracownika),  constraint fk_id_klubu foreign key (id_klubu) references kluby_lekkoatletyczne(id_klubu) on delete cascade);
create table dyscypliny(  id_dyscypliny number default seq_dyscypliny.nextval not null,  nazwa varchar2(20) not null,    constraint pk_id_dyscypliny primary key (id_dyscypliny));
create table trenerzy_dyscypliny(  id_trenera number not null,  id_dyscypliny number not null,    constraint pk_trenerzy_dyscypliny primary key (id_dyscypliny,id_trenera),  constraint fk_id_trenera foreign key (id_trenera) references trenerzy(id_trenera) on delete cascade,  constraint fk_id_dyscypliny foreign key (id_dyscypliny) references dyscypliny(id_dyscypliny) on delete cascade);
create table zawodnicy(  id_zawodnika number default seq_zawodnicy.nextval not null ,  imie varchar2(15) not null,  nazwisko varchar2(25) not null,  pesel number(11) not null,  plec char(1) not null,  id_klubu number not null,  id_trenera number not null,    constraint pk_id_zawodnika primary key (id_zawodnika),  constraint fk_id_klubu foreign key (id_klubu) references kluby_lekkoatletyczne(id_klubu) on delete cascade,  constraint fk_id_trenera foreign key (id_trenera) references trenerzy(id_trenera),  constraint chck_plec check (plec in ('K','M')));
create table obiekty(  );
create table treningi(  id_treningu number default seq_treningi.nextval not null,  id_obiektu number,  id_trenera number not null,  id_masazysty number,    constraint pk_id_treningu primary key (id_treningu)  );
